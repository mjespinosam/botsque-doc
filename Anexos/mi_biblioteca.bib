  title = {People, {{Means}}, and {{Activities}}: {{A Conceptual Framework}} for {{Realizing}} the {{Educational Potential}} of {{Makerspaces}}},
  shorttitle = {People, {{Means}}, and {{Activities}}},
  author = {Hira, Avneet and link will open in a new window Link to external site, this and Hynes, Morgan M.},
  editor = {Szalay, Paul S.},
  date = {2018},
