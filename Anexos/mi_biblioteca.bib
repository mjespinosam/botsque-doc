@article{vaageWhatEthicsBioart2016,
  title = {What {{Ethics}} for {{Bioart}}?},
  author = {Vaage, Nora S.},
  date = {2016-04-01},
  journaltitle = {NanoEthics},
  shortjournal = {Nanoethics},
  volume = {10},
  pages = {87--104},
